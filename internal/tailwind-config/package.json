{
  "name": "@kk-project/tailwind-config",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./postcss": {
      "types": "./src/postcss.config.ts",
      "import": "./dist/postcss.config.mjs",
      "require": "./dist/postcss.config.cjs",
      "default": "./dist/postcss.config.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "pnpm unbuild --stub"
  },
  "dependencies": {
    "@csstools/postcss-color-mix-function": "^3.0.1",
    "@iconify/json": "^2.2.240",
    "@iconify/tailwind": "^1.1.3",
    "@lorenzopalaia/tailwind-hero-patterns": "^1.0.1",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.14",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.5",
    "postcss": "^8.4.41",
    "postcss-antd-fixes": "^0.2.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.0.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-primeui": "^0.3.4"
  },
  "devDependencies": {
    "@types/postcss-import": "^14.0.3"
  }
}
