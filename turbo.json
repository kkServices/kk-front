{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "pnpm-lock.yaml",
    "**/.env.*local",
    "**/tsconfig*.json",
    "internal/tailwind-config/src/**/*.ts"
  ],
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "stub": {},
    "build:prepare": {
      "dependsOn": ["stub"],
      "outputs": []
    },
    "dev": {
      "dependsOn": [],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "outputs": []
    }
  }
}
